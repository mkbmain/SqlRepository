<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <AssemblyName>Mkb.EF.Repo</AssemblyName>
        <RootNamespace>Mkb.EF.Repo</RootNamespace>
        <Title>Mkb.EF.Repo</Title>
        <Authors>Mkb</Authors>
        <Description>Simple sql repo requires db context to be parsed in details at https://github.com/mkbmain/SqlRepository</Description>
        <PackageProjectUrl>https://github.com/mkbmain/SqlRepository</PackageProjectUrl>
        <RepositoryUrl>https://github.com/mkbmain/SqlRepository</RepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.EntityFrameworkCore" Version="5.0.5" />
    </ItemGroup>

</Project>
